/**app.wxss**/
.container {
  height: 100vh;
  /* overflow: hidden; */
  box-sizing: border-box;
} 
/* padding margin */
.mt20{
  margin-top: 20px;
}
.mb10{
  margin-bottom: 10px;
}
.p10{
  padding: 10px;
}
.pl10{
  padding-left: 10px;
}
.pb48{
  padding-bottom: 48px;
}
/* 颜色 */
.color-blue-dark{
  color: #0061AC;
}
.color-placeholder{
    color: #D7D7D7;
}
.bg-fff{
  background-color: #fff;
}
/* iconfont */
.icon-user{
  background-image: url("data:image/svg+xml;base64,");
}
.icon-lock{
  background-image: url("data:image/svg+xml;base64,");
}
.icon{
  background-repeat: no-repeat;
  background-size: 100%;
  width: 20px;
  height: 20px;
  display: inline-block;
  margin-top: 3px;
}
.input-wrapper{
  display: flex;
  flex-direction: row;
  padding: 10px;
  justify-content: center;
  border-bottom: 1px solid #fff;
}
.input-wrapper .input{
  flex: 1;
  padding-left: 10px;
  font-size: 14px;
  margin-top: 5px;
}
.card{
  background-color: #fff;
  border-radius: 10px;
  margin: 10px;
}
/* 宫格 */
.grid-wrapper{
  box-sizing: border-box;
}
.grid-item{
  padding: 10px;
  width: 25%;
  display: inline-block;
  text-align: center;
  box-sizing: border-box;
}
.grid-item-icon{
  width: 42px;
  height: 42px;
}
.grid-item-text{
  font-size: 12px;
  color: #333;
}
/* flex */
.flex-row{
  display: flex;
  flex-direction: row;
  align-content: center;
  align-items: center;
}
.flex-column{
  display: flex;
  flex-direction: column;
}
.flex-column-justify-center{
  justify-content: center;
}
.flex-1{
  flex: 1;
}
/* 清除浮动 */
.clearfix:after {
  content: '';
  height: 0;
  line-height: 0;
  display: block;
  visibility: hidden;
  clear: both;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
/* 箭头 */
.arrow-back{
  width: 9px;
  height: 15px;
}
.arrow{
  width: 10px;
  height: 16px;
  margin-top: 4px;
  transition: 0.5s;
}
.arrow-active{
  transform: rotate(90deg);
  transition: 0.5s;
}
.none{
  display: none;
}
.show-accordion-panel{
  height: max-content;
  transition: 3s;
}
.text-center{
  text-align: center;
}
.search-bar .weui-search-bar{
  background-color: transparent;
}
.search-bar .weui-search-bar__form{
  border-radius: 50px;
}
.search-bar .weui-search-bar__label{
  border-radius: 50px;
}
.table-form-cell{
  padding: 0;
}
.table-form-cell .weui-cell::before{
  height: 0;
}
.table-form-cell .weui-label{
  font-size: 15px;
  padding: 16px;
  color: #AEAEAE;
  text-align: left;
  border-right: 1px solid #f7f5f5;
}
.table-form-cell .label-right{
  font-size: 15px;
  text-align: right;
  padding-right: 16px;
  color: #333;
}
.table-form-cell .label-right .weui-select{
  width: inherit;
}
.table-form-cell .label-right .weui-input{
  width: calc(100% - 25px);
} 
.table-form .weui-cells__group{
  background-color: #fff;
}
.table-form .weui-cells__title {
  padding: 10px;
  color: #333;
}